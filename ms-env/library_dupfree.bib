author = {Chen, Biqing and Liang, Hao and Yuan, Xiaomin and Hu, Yingying and Xu, Miao and Zhao, Yating and Zhang, Binfen and Tian, Fang and Zhu, Xuejun},
doi = {10.1101/2020.03.16.20037168},
file = {:home/tpsmith/Documents/COVID/Reading/COVID-19/Chen{\_}2020{\_}MedRxiv.pdf:pdf},
journal = {medRxiv},
pages = {2020.03.16.20037168},
title = {{Roles of meteorological conditions in COVID-19 transmission on a worldwide scale}},
url = {https://www.medrxiv.org/content/10.1101/2020.03.16.20037168v1},
volume = {11},
year = {2020}
}
