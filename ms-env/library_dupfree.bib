doi = {10.1111/2041-210X.12168},
file = {:home/tpsmith/Documents/Library/Beckmann{\_}2014{\_}MethodsEcolEvol.pdf:pdf},
issn = {2041210X},
journal = {Methods in Ecology and Evolution},
keywords = {Aura-OMI,Bioclim,CliMond,Climatology,Global data,Macroecology,Species distribution modelling,UV-B radiation,WorldClim},
